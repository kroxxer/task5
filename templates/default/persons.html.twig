{% for person in persons %}
    <tr class="person">
        <td>{{ person.id }}</td>
        <td>{{ person.identity }}</td>
        <td>{{ person.name }}</td>
        <td>{{ person.address }}</td>
        <td>{{ person.phoneNumber }}</td>
    </tr>
{% else %}
    <tr>
        <td colspan="6">no records found</td>
    </tr>
{% endfor %}